<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlowLayout">
        <attr name="fl_line_align" format="enum">
            <enum name="top" value="0"></enum>
            <enum name="center" value="1"></enum>
            <enum name="bottom" value="2"></enum>
        </attr>
        <attr name="fl_content_align" format="enum">
            <enum name="left" value="0"></enum>
            <enum name="center" value="1"></enum>
            <enum name="right" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>
